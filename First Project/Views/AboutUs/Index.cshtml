@model IEnumerable<First_Project.Models.AboutU>

@{
    ViewData["Title"] = "Index";
        Layout="~/Views/Shared/AdminLayout.cshtml";


}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FirstDescreption)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SecondDescreption)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ImgPath)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstDescreption)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SecondDescreption)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImgPath)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
